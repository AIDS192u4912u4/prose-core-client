    <os>Browser</os>
    <os>macOS</os>
    <os>iOS</os>
    <os>Android</os>
    <os>Linux</os>
    <os>Windows</os>

    <repository>
      <GitRepository>
        <browse rdf:resource="https://github.com/prose-im/prose-core-client"/>
